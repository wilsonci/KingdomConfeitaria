  <!-- Target para copiar imagens durante a publicação -->
  <Target Name="CopyImagesToPublish" AfterTargets="CopyAllFilesToSingleFolderForPackage">
    <ItemGroup>
      <ImagesFiles Include="Images\**\*.*" Exclude="Images\**\*.ps1;Images\**\*.py;Images\**\*.txt;Images\**\*.md" />
    </ItemGroup>
    <Message Text="Copiando imagens para publicação..." Importance="high" />
    <Copy SourceFiles="@(ImagesFiles)" DestinationFolder="$(PackageLocation)Images\%(RecursiveDir)" SkipUnchangedFiles="true" Condition="'$(PackageLocation)' != ''" />
    <Copy SourceFiles="@(ImagesFiles)" DestinationFolder="$(PublishUrl)Images\%(RecursiveDir)" SkipUnchangedFiles="true" Condition="'$(PublishUrl)' != '' And '$(PackageLocation)' == ''" />
